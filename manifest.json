{
  "manifest_version": 3,
  "name": "AutoBlockout",
  "description": "Extension to block accounts in TikTok and Instagram in bulk",
  "version": "1.0",
  "host_permissions": ["https://www.instagram.com/*", "https://www.tiktok.com/*", "https://raw.githubusercontent.com/mfrashad/AutoBlockout/main/blacklist.json"],
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  }
}
